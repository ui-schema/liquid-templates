<a
    data-schema="uischema.org/Feature"
    href="{{ link.href }}"    
    target="{{ link.target }}"    
>
    {{# image }}
        {{> Image }}
    {{/ image }}

    {{# heading }}
        <h3>{{ heading }}</h3>
    {{/ heading }}
    
    {{# subheading }}
        <h4>{{ subheading }}</h4>
    {{/ subheading }}}
    
    {{# body }}
        <p>{{ body }}</p>
    {{/ endif }}
</a>
