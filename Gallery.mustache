<section data-ui-schema="uischema.org/Gallery" class="ui-gallery{{# options }}{{# isCarousel }} is-carousel{{/ isCarousel }}{{# isGrid }} is-grid{{/ isGrid }}{{# useCoverLinks }} use-cover-links{{/ useCoverLinks }}{{# useSquares }} use-squares{{/ useSquares }}{{# columns }} {{# xs }} columns-xs-{{ . }}{{/ xs }}{{# sm }} columns-sm-{{ . }}{{/ sm }}{{# md }} columns-md-{{ . }}{{/ md }}{{# lg }} columns-lg-{{ . }}{{/ lg }}{{# xl }} columns-xl-{{ . }}{{/ xl }}{{/ columns }}{{/ options }}">
    <div class="ui-container">
        {{# heading }}
            <h2 class="ui-gallery__heading">{{ . }}</h2>
        {{/ heading }}
        
        {{# subheading }}
            <h3 class="ui-gallery__subheading">{{ . }}</h3>
        {{/ subheading }}
        
        {{# body }}
            <div class="ui-gallery__body">{{{ . }}}</div>
        {{/ body }}
    </div>

    <div class="ui-gallery__items{{# options.isContained }} ui-container{{/ options.isContained }}">
        {{# items }}
            <div class="ui-gallery__item">{{> Feature }}</div>
        {{/ items }}
    </div>
</section>
